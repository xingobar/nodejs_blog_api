{
  "name": "blog_api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "ts-dev": "ts-node ./src/index.ts",
    "dev": "NODE_PATH=./src nodemon ./src/index.ts",
    "dev:test": "NODE_PATH=./src  nodemon ./src/index.ts",
    "test": "NODE_PATH=./src mocha --require ts-node/register test/*.ts --timeout 500000 --exit ",
    "typeorm": "NODE_PATH=./src node --require ts-node/register ./node_modules/typeorm/cli.js --config src/config/ormconfig.ts",
    "typeorm:test": "NODE_PATH=./src node --require ts-node/register ./node_modules/typeorm/cli.js --config src/config/ormconfig.ts --connection test",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "seed:config": "node --require ts-node/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "node --require ts-node/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:name": "node --require ts-node/register ./node_modules/typeorm-seeding/dist/cli.js seed -s"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.4",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@decorators/di": "^1.0.3",
    "@decorators/express": "^2.6.0",
    "@types/chai": "^4.2.19",
    "@types/expect": "^24.3.0",
    "@types/faker": "^5.5.6",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^8.2.2",
    "@types/sqlite3": "^3.1.7",
    "@types/supertest": "^2.0.11",
    "argon2": "^0.28.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "celebrate": "^14.0.0",
    "chai": "^4.3.4",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-status": "^1.5.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mocha": "^9.0.1",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3",
    "ts-mocha": "^8.0.0",
    "ts-transformer-keys": "^0.4.3",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.34",
    "typeorm-seeding": "^1.6.1",
    "typeorm-typedi-extensions": "^0.4.1"
  }
}
